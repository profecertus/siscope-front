{
  "name": "ng-devui-admin",
  "version": "1.0.0",
  "license": "MIT",
  "description": "DevUI Admin based on Angular and DevUI components",
  "repository": {
    "type": "git",
    "url": "https://github.com/DevCloudFE/ng-devui-admin"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --port 8001",
    "build": "ng build",
    "build:website": "ng build --configuration production --base-href /siscope/ --output-path dist/siscope",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "tslint": "tslint --fix -c ./tslint.json 'src/**/*{.ts}'",
    "prettier": "prettier --config ./.prettierrc --write \"src/app/pages/**/**/*.html\"",
    "stylelint": "stylelint \"src/app/pages/**/**/*.{scss,css}\" --config ./.stylelintrc.json --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/cdk": "^16.0.1",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@devui-design/icons": "^1.3.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "color": "^3.1.3",
    "echarts": "5.2.1",
    "gridstack": "^6.0.0",
    "ng-devui": "^16.1.0",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.8.0",
    "tslib": "^2.3.0",
    "validate-ruc": "^1.0.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.1",
    "@angular/cli": "~16.0.1",
    "@angular/compiler-cli": "^16.0.0",
    "@types/jasmine": "~4.3.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~4.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "protractor": "~7.0.0",
    "shelljs": "^0.8.4",
    "stylelint": "^13.13.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.19.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~5.0.4"
  }
